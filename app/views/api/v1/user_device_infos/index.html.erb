<p id="notice"><%= notice %></p>

<h1>Api V1 User Device Infos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Os</th>
      <th>Device type</th>
      <th>Device model</th>
      <th>System version</th>
      <th>App version</th>
      <th>Device locale</th>
      <th>Bundle device</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_user_device_infos.each do |api_v1_user_device_info| %>
      <tr>
        <td><%= api_v1_user_device_info.user %></td>
        <td><%= api_v1_user_device_info.os %></td>
        <td><%= api_v1_user_device_info.device_type %></td>
        <td><%= api_v1_user_device_info.device_model %></td>
        <td><%= api_v1_user_device_info.system_version %></td>
        <td><%= api_v1_user_device_info.app_version %></td>
        <td><%= api_v1_user_device_info.device_locale %></td>
        <td><%= api_v1_user_device_info.bundle_device %></td>
        <td><%= link_to 'Show', api_v1_user_device_info %></td>
        <td><%= link_to 'Edit', edit_api_v1_user_device_info_path(api_v1_user_device_info) %></td>
        <td><%= link_to 'Destroy', api_v1_user_device_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 User Device Info', new_api_v1_user_device_info_path %>
