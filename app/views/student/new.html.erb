<%= form_for :student, url: new_student_path do |f| %>



<%= f.label :first_name %>
<%= f.text_field :first_name %>

<%= f.label :last_name %>
<%= f.text_field :last_name %>

<%= f.label :address %>
<%= f.text_field :address%>

<%= f.label :section %>
<%= f.text_field :section_id %>

<%= f.submit %>
<% end %>